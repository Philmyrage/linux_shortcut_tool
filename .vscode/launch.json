{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug C++",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/linux_shortcut_tool",
            "args": [
                "-n",
                "test",
                "-te",
                "false",
                "-e",
                "/mnt/Data/Godot/bin/linux/godot",
                "-img",
                "/mnt/Data/Godot/icon.png",
                "-c",
                "This is a test comment",
                "-k",
                "test demo",
                "-cat",
                "Utility Application",
                "-typ",
                "Application"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": true
            }
        }
    ]
}